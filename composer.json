{
  "name": "ajthinking/laravel-postgis",
  "description": "Postgis extensions for laravel. Aims to make it easy to work with geometries from laravel models",
  "require": {
    "php": ">=7.1",
    "illuminate/database": "^9.0|^10.0",
    "geo-io/wkb-parser": "^1.0",
    "jmikola/geojson": "^1.0"
  },
  "require-dev": {
    "illuminate/pagination": "^9.0",
    "phpunit/phpunit": "^9.0.0",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Ajthinking\\LaravelPostgis\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ajthinking\\LaravelPostgis\\Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Haza",
      "email": "peter.haza@gmail.com"
    },
    {
      "name": "Nicholas Barrett",
      "email": "njbarrett7@gmail.com"
    },
    {
      "name": "Max Matteo Staack",
      "email": "maxmatteostaack@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Ajthinking\\LaravelPostgis\\DatabaseServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  }
}
